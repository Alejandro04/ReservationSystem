{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@now/node-server"
        },
        {
            "src": "./src",
            "use": "@now/static"
        }
    ],
    "routes": [
        {
            "src": "/.*",
            "dest": "/index.js"
        }
    ]
}