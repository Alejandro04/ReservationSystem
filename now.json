{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@now/node-server",
            "config": {
                "includeFiles": ["src/*"],
                "bundle": false
              }
        }
    ],
    "routes": [
        {
            "src": "/.*",
            "dest": "/index.js"
        }
    ]
}